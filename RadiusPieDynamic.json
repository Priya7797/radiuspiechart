{
	"name": "Radius Pie chart 2",
	"description": "The first step in creating a minamilistic KPI tile.  Now with properties",
	"eula": "",
	"vendor": "",
	"license": "",
	"id": "com.sap.sample.piechar2",
	"newInstancePrefix": "piechart2",
	"version": "0.0.1",
	"icon": "data:image/png;base64,",
	"webcomponents": [
        {
			"kind": "main",
			"tag": "com-sap-sample-piechart1",
            "url": "https://priya7797.github.io/radiuspiechart/radiuspiechart.js",
            "integrity": "",
            "ignoreIntegrity" : true
        }
    ],
	"properties": {
	
		"datasourceString": {
			"type": "string",
			"description": "DataSource ResultSet String",
			"default": "{}"
		}
	},
	"methods": {
		"setDataSourceString": {
			"description": "Sets the datasource for the chart",
			"parameters": [
				{
					"name": "resultsetString",
					"type": "string",
					"description": "DataSource string for the widget"
				}
			],
			"body": "this.datasourceString = resultsetString;"
		},
		"getDataSourceString": {
			"returnType": "string",
			"description": "Gets the datasource for the chart",
			"body": "return this.datasourceString;"
		}
	},
	"events": {
	}
}
