{
	"name": "Radius Pie chart",
	"description": "The first step in creating a minamilistic KPI tile.  Now with properties",
	"eula": "",
	"vendor": "",
	"license": "",
	"id": "com.sap.sample.piechart",
	"newInstancePrefix": "piechart",
	"version": "1.0.0",
	"icon": "data:image/png;base64,",
	"webcomponents": [
        {
			"kind": "main",
			"tag": "com-sap-sample-piechart",
            "url": "https://priya7797.github.io/radiuspiechart/demopiechart.js",
            "integrity": "",
            "ignoreIntegrity" : true
        },
		{
			"kind": "styling",
			"tag": "com-sap-sample-piechart-styling",
			"url": "https://priya7797.github.io/radiuspiechart/demopiechartstyling.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-sap-sample-piechart-builder",
			"url": "https://priya7797.github.io/radiuspiechart/demupiechartbuilder.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
    ],
	"properties": {
	
		"statusCheckBox": {
			"type": "boolean",
			"description": "DataSource ResultSet String",
			"default": false
		},
		"legendvalue": {
			"type": "boolean",
			"description": "shows legend value outside.",
			"default": false
		},

		"radiovalue": {
			"type": "boolean",
			"description": "shows value",
			"default": false
		},

		"radiopercent": {
			"type": "boolean",
			"description": "shows percentage",
			"default": false
		},

		"radiocombination": {
			"type": "boolean",
			"description": "shows combination",
			"default": false
		},

		"datasourceString": {
			"type": "string",
			"description": "DataSource ResultSet String",
			"default": "{}"
		},
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "#67b7dc;#a367dc;#dc6788"
		},
		"title": {
			"type": "string",
			"description": "Title of the Chart",
			"default": ""
		},
		"titlefontsize": {
			"type": "integer",
			"description": "Font size of the chart title in pixels",
			"default": 24
		},
		"width": {
			"type": "integer",
			"default": 400
		},
		"height": {
			"type": "integer",
			"default": 400
		}
		
	},
	"methods": {
		"setDataSourceString": {
			"description": "Sets the datasource for the chart",
			"parameters": [
				{
					"name": "resultsetString",
					"type": "string",
					"description": "DataSource string for the widget"
				}
			],
			"body": "this.datasourceString = resultsetString;"
		},
		"getDataSourceString": {
			"returnType": "string",
			"description": "Gets the datasource for the chart",
			"body": "return this.datasourceString;"
		},
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		},
		"setTitle": {
			"description": "Sets the tile of the chart.",
			"parameters": [
				{
					"name": "newTitle",
					"type": "string",
					"description": "The new title of the chart."
				}
			],
			"body": "this.title = newTitle;"
		},
		"getTitle": {
			"returnType": "string",
			"description": "Returns the chart title",
			"body": "return this.title;"
		},
		"setTitleFontSize": {
			"description": "Sets the font size for the chart title.",
			"parameters": [
				{
					"name": "newFontSize",
					"type": "integer",
					"description": "The new title of the chart."
				}
			],
			"body": "this.titlefontsize = newFontSize;"
		},
		"getTitleFontSize": {
			"returnType": "integer",
			"description": "Returns the font size for the chart title.",
			"body": "return this.titlefontsize;"
		},
		
		"setstatusCheckBox": {
			"description": "Set CheckBox status",
			"parameters": [
				{
					"name": "newStatus",
					"type": "boolean",
					"description": "Status Change"
				}
			],
			"body": "this.statusCheckBox = newStatus;"
		},
		
		"getstatusCheckBox": {
			"returnType": "boolean",
			"description": "Set CheckBox status",
			"body": "return this.statusCheckBox;"
		},
		"setlegendvalue": {
			"description": "Set legent value",
			"parameters": [
				{
					"name": "newlegendValue",
					"type": "boolean",
					"description": "Status Change"
				}
			],
			"body": "this.legendvalue = newlegendValue;"
		},
		
		"getlegendvalue": {
			"returnType": "boolean",
			"description": "Set CheckBox status",
			"body": "return this.legendvalue;"
		},



		"setradiovalue": {
			"description": "Sets value of radio button",
			"parameters": [
				{
					"name": "newradiovalue",
					"type": "boolean",
					"description": "Status Change"
				}
			],
			"body": "this.radiovalue = newradioValue;"
		},
		
		"getradiovalue": {
			"returnType": "boolean",
			"description": "get radio value",
			"body": "return this.radiovalue;"
		},

		"setradiopercent": {
			"description": "Set percentage of legend",
			"parameters": [
				{
					"name": "newradiopercent",
					"type": "boolean",
					"description": "percent"
				}
			],
			"body": "this.radiopercent = newradiopercent;"
		},
		
		"getradiopercent": {
			"returnType": "boolean",
			"description": "get percentage of legend",
			"body": "return this.radiopercent;"
		},

		"setradiocombination": {
			"description": "Set combination of legend value",
			"parameters": [
				{
					"name": "newradiocombination",
					"type": "boolean",
					"description": "Status Change"
				}
			],
			"body": "this.radiocombination = newradiocombination;"
		},
		
		"getradiocombination": {
			"returnType": "boolean",
			"description": "get both value and percentage",
			"body": "return this.radiocombination;"
		}
		
		
	},
	"events": {
	}
}